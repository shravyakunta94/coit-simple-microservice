pipeline{
 agent any

stages {
   stage('build') {
steps {
    cd coit-backend1 && mvn install
}
}
 stage('docker-build') {
steps {
    cd coit-backend1
    docker build -t shravyakunta94/coit-backend1:latest .
}
}
 stage('docker-push') {
steps {
    docker push shravyakunta94/coit-backend1:latest
}
}
}
}
